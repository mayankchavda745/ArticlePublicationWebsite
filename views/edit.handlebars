{{#each data}}
<form>
  <div class="form-group">
    FirstName:
    <input type="text" class="form-control" id="efname" value="{{this.fname}}">
  </div>


  <div class="form-group">
    LastName:
    <input type="text" class="form-control" id="elname" value="{{this.lname}}">
  </div>


  <div class="form-group">
    Email:
    <input type="text" class="form-control" id="eemail" value="{{this.email}}">
  </div>

  <div class="form-group">
    MobileNo:
    <input type="text" class="form-control" id="emobileno" value="{{this.mobileno}}">
  </div>

  <div class="form-group">
    Message:
    <input type="text" class="form-control" id="emessage" value="{{this.message}}">
  </div>
  <br>
  Gender:
  <p>
    <label><input type="radio" name="gender1" value="male">Male</label>
    <label><input type="radio" name="gender1" value="female">Female</label>
  </p>

  <div class="form-group">
    <input class="form-check-input" type="checkbox" value="" name="etick" required>I accept the term and condition
  </div>

  <button type="submit" id="edit" class="btn btn-primary"><a href="/usermessage">Update</a></button>

   <input type="text" id="id" value="{{this.id}}" hidden>
</form>
{{/each}}